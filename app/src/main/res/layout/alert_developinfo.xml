<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/developInfoAuthor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="作者:feijiang00"
        android:textColor="@color/dark"
        android:textSize="16sp"
        android:padding="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/developInfoWeb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="个人网站:https://www.feijiang00.top"
        android:textColor="@color/dark"
        android:textSize="16sp"
        android:padding="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/developInfoAuthor" />

    <TextView
        android:id="@+id/developInfoOrigin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="书山有路勤为径，学海无涯苦作舟"
        android:textColor="@color/dark"
        android:textSize="16sp"
        android:padding="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/developInfoWeb" />

    <TextView
        android:id="@+id/developInfoOther"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="(o゜▽゜)o☆"
        android:textColor="@color/dark"
        android:textSize="16sp"
        android:padding="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/developInfoOrigin" />
</androidx.constraintlayout.widget.ConstraintLayout>